<div class="orderBox row">
  <div class="col s12">
    <h1>4US</h1>
   
    <form class="create-form">
      <h4><i class="fas fa-file"></i>Please type in your burger selection</h4>
      <div class="form-group">
        <input type="text" id="bb" name="name" placeholder="Enter your burger">
      </div>

      <button class="btn waves-effect waves-light btn-medium" type="submit" name="action">Submit</button>
    </form>
  </div>
  {{!-- <div class="row"> --}}
    <div class="col s12">
      <div class="orderedbox">
        <h3>Burgers ordered!</h3>
        <ul>
          {{#each burgerdb}}
          {{#unless devoured}}
          {{> b/b-block devoured=true}}
          {{/unless}}
          {{/each}}
        </ul>
      </div>
    </div>
    <div class="col s12">

      <div class="orderagainbox">
        <h2>Would you like to order again?</h2>
        <ul>
          {{#each burgerdb}}
          {{#if devoured}}
          {{> b/b-block devoured=false}}
          {{/if}}
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
{{!-- </div> --}}